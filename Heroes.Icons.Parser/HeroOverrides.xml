<?xml version="1.0" encoding="utf-8" ?>
<HeroOverrides>
  <CHero id="Alarak">
    <!--Cooldown and mana cost are deducted after charge release-->
    <Ability id="AlarakDeadlyCharge">
      <Override>
        <Tooltip.Energy.EnergyCost>&lt;d ref="Effect,AlarakDeadlyChargeUnleashModifyUnit,Cost[0].CooldownTimeUse"/></Tooltip.Energy.EnergyCost>
        <Tooltip.Cooldown.CooldownValue>&lt;d ref="-Effect,AlarakDeadlyChargeUnleashModifyUnit,VitalArray[Energy].Change"/></Tooltip.Cooldown.CooldownValue>
      </Override>
    </Ability>
  </CHero>
  <CHero id="Alexstrasza">
    <!--Dragonqueen abilities tied to Dragonqueen form ability-->
    <Ability id="AlexstraszaGiftOfLife">
      <Override>
        <Tooltip.Life.LifeCost>&lt;d ref="-100*Accumulator,AlexstraszaGiftOfLifeCostAccumulator,Ratio"/></Tooltip.Life.LifeCost>
        <Tooltip.Life.IsLifePercentage value="true" />
      </Override>
    </Ability>
    <Ability id="AlexstraszaBreathOfLife" valid="true">
      <Override>
        <ParentLink value="AlexstraszaDragonqueen" />
      </Override>
    </Ability>
    <Ability id="AlexstraszaPreservation" valid="true">
      <Override>
        <ParentLink value="AlexstraszaDragonqueen" />
      </Override>
    </Ability>
    <Ability id="AlexstraszaWingBuffet" valid="true">
      <Override>
        <ParentLink value="AlexstraszaDragonqueen" />
      </Override>
    </Ability>
    <Ability id="AlexstraszaCleansingFlameDragonqueen" valid="true">
      <Override>
        <ParentLink value="AlexstraszaDragonqueen" />
      </Override>
    </Ability>
    <SubHeroUnit value="Hero">
      <Unit id="HeroAlexstraszaDragon">
        <Name value="Dragonqueen" />
        <ShortName value="Dragonqueen" />
      </Unit>
    </SubHeroUnit>
  </CHero>
  <CHero id="Arthas">
