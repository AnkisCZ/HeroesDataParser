<?xml version="1.0" encoding="utf-8" ?>
<HeroOverrides>
  <CHero id="Abathur">
    <HeroUnit id="AbathurSymbiote"></HeroUnit>
  </CHero>
  <CHero id="Falstad">
    <Portrait>
      <Override>
        <HeroSelectPortraitFileName value="storm_ui_ingame_heroselect_btn_gryphon_rider.dds" />
      </Override>
    </Portrait>
  </CHero>
  <CHero id="Dryad" name="Lunara" alt="Lunara">
    <Ability id="DryadGallopingGait">
      <Override>
        <CooldownTooltip value="" />
      </Override>
    </Ability>
  </CHero>
  <CHero id="TestHero">
    <Ability id="TestHeroIllusionMaster" button="TestHeroAdvancingStrikes" add="true" />
    <Ability id="TestUnitCallUnit" valid="true">
      <Override>
        <ParentLink value="HeroTestUnit" />
      </Override>
    </Ability>
    <HeroAbilArray button="TestHeroEssenseCollectionStore">
      <Set value="TestHeroEssenseCollection" />
    </HeroAbilArray>
  </CHero>
  <CHero id="Ragnaros">
    <!--Molten Core abilities-->
    <LinkedAbilities>
      <Ability id="RagnarosBigRagMoltenSwing" element="CAbilEffectTarget" />
      <Ability id="RagnarosBigRagMeteorShower" element="CAbilEffectTarget" />
      <Ability id="RagnarosBigRagExplosiveRune" element="CAbilEffectTarget" />
    </LinkedAbilities>
    <Ability id="RagnarosBigRagMoltenSwing" valid="true">
      <Override>
        <ParentLink value="RagnarosBigRag" />
        <AbilityType value="Q" />
      </Override>
    </Ability>
    <Ability id="RagnarosBigRagMeteorShower" valid="true">
      <Override>
        <ParentLink value="RagnarosBigRag" />
        <AbilityType value="W" />
      </Override>
    </Ability>
    <Ability id="RagnarosBigRagExplosiveRune" valid="true">
      <Override>
        <ParentLink value="RagnarosBigRag" />
        <AbilityType value="E" />
      </Override>
    </Ability>
    <HeroUnit id="RagnarosBigRag"></HeroUnit>
  </CHero>
</HeroOverrides>
